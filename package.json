{
  "name": "kad-quasar",
  "version": "1.2.4",
  "description": "publish/subscribe extension system for kad",
  "main": "index.js",
  "directories": {
    "doc": "doc",
    "example": "example",
    "test": "test"
  },
  "scripts": {
    "test": "npm run testsuite && npm run linter",
    "testsuite": "./node_modules/.bin/mocha test/** --recursive",
    "coverage": "./node_modules/.bin/istanbul cover -x 'lib/logger.js' ./node_modules/.bin/_mocha -- --recursive",
    "linter": "./node_modules/.bin/jshint --config .jshintrc ./index.js ./lib ./test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kadtools/kad-quasar.git"
  },
  "engines": {
    "node": ">=6.9.1"
  },
  "keywords": [
    "kad",
    "publish",
    "subscribe",
    "pubsub",
    "sensor",
    "network"
  ],
  "author": "Gordon Hall <gordon@gordonwritescode.com>",
  "contributors": [
    {
      "name": "littleskunk",
      "url": "https://github.com/littleskunk"
    }
  ],
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/kadtools/kad-quasar/issues"
  },
  "homepage": "https://github.com/kadtools/kad-quasar#readme",
  "devDependencies": {
    "chai": "^3.4.1",
    "coveralls": "^2.11.6",
    "istanbul": "^0.4.2",
    "jshint": "^2.9.1",
    "mocha": "^2.3.4",
    "sinon": "^1.17.2"
  },
  "dependencies": {
    "async": "^1.5.2",
    "atbf": "^1.1.0",
    "kad": "^1.6.0",
    "knuth-shuffle": "^1.0.1",
    "lru-cache": "^4.0.1",
    "merge": "^1.2.0",
    "uuid": "^3.0.0"
  }
}
